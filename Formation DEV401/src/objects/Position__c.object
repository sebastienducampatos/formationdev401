        <fullName>Every_Position_should_have_a_Hiring_Mgr</fullName>
        <active>true</active>
        <description>Every position record should have a hiring manager</description>
        <errorConditionFormula>LEN( Hiring_Manager__c ) = 0</errorConditionFormula>
        <errorDisplayField>Hiring_Manager__c</errorDisplayField>
        <errorMessage>Every position record must have a hiring manager</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Temp_Position_Validation</fullName>
        <active>true</active>
        <description>Temporary positions require a value for Duration between 1 and 365 days.</description>
        <errorConditionFormula>ISPICKVAL( Type__c , &quot;Temp&quot;)  &amp;&amp; ( BLANKVALUE( Duration__c , 0)  &lt;= 0 ||  Duration__c  &gt;= 365 )</errorConditionFormula>
        <errorDisplayField>Duration__c</errorDisplayField>
        <errorMessage>Temporary positions require a value for Duration between 1 and 365 days.</errorMessage>
    </validationRules>
