    <validationRules>
        <fullName>Zip_Code_consistent_with_States</fullName>
        <active>true</active>
        <description>Validates candidate Zip/Postal Code by looking up the first five characters of the value in a custom object called Zip_Code__c.
Error if the Zip Code is not found or the candidate State does not match the corresponding state in the object.</description>
        <errorConditionFormula>AND( VLOOKUP(  $ObjectType.Zip_Code__c.Fields.State__c,  $ObjectType.Zip_Code__c.Fields.Name, LEFT(  Zip_Postal_Code__c, 5 ) )  &lt;&gt;  State_Province__c, NOT (  Batch_Load_Item__c ) )</errorConditionFormula>
        <errorDisplayField>Zip_Postal_Code__c</errorDisplayField>
        <errorMessage>Candidate Zip Code does not exist in specified state.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Zip_code_must_be_valid_US_Postal_Code</fullName>
        <active>true</active>
        <description>Validates that the candidate Zip/postal code is in 99999 or 99999-9999 format if country is USA or US.</description>
        <errorConditionFormula>( Country__c == &quot;USA&quot; || Country__c == &quot;US&quot;) &amp;&amp; NOT( REGEX( Zip_Postal_Code__c , &quot;\\d{5}(-\\d{4})?&quot;) )</errorConditionFormula>
        <errorDisplayField>Zip_Postal_Code__c</errorDisplayField>
        <errorMessage>Zip code must be in 99999 or in 99999-9999 format for US.</errorMessage>
    </validationRules>
</CustomObject>
